package ch02_pjt_01;

public class MyCalculator {

	public void calAdd(int fNum, int sNum, ICalculator caculator) {
		int value = caculator.doOperation(fNum, sNum);
		System.out.println("result : " + value);
	}
		
	public void calSub(int fNum, int sNum, ICalculator caculator) {
		int value = caculator.doOperation(fNum, sNum);
		System.out.println("result : " + value);
	}
			
	public void calMul(int fNum, int sNum, ICalculator caculator) {
		int value = caculator.doOperation(fNum, sNum);
		System.out.println("result : " + value);
	}
		
	public void calDiv(int fNum, int sNum, ICalculator caculator) {
		int value = caculator.doOperation(fNum, sNum);
		System.out.println("result : " + value);
	}
}
